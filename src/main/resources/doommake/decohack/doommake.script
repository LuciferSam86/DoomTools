/* ------------------------------------------------------------------------ */

#define SRC_DECOHACK        "/decohack/main.dh"

/**
 * Calls DECOHack.
 * Creates patch.
 */
check function doPatch(budget) {
	initBuild();
	decohack(
		file(getSourceDirectory() + SRC_DECOHACK), 
		file(getBuildDirectory() + "/" + getPatchFile()), 
		budget
	);
}

